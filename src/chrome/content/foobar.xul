<?xml-stylesheet href="chrome://foobar/content/foobar.css" type="text/css"?>   
<!DOCTYPE window SYSTEM "chrome://foobar/locale/foobar.dtd">

<overlay id="foobar2000control" 
  xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
  xmlns:nc="http://home.netscape.com/NC-rdf#">
  
  <script type="application/x-javascript" src="chrome://foobar/content/foobar.js"/>

  <toolbarpalette id="BrowserToolbarPalette">
    <toolbaritem id="foobar_stop_item"
      title="&foobar.stop;">
      <toolbarbutton id="foobar_stop"
        class="foobarcontrol"
        tooltiptext="&foobar.stop;"
        label="&foobar.toolbar.stop;"
        minimised="false"
        oncommand="return foobar.docommand('stop');"/>
    </toolbaritem>
    <toolbaritem id="foobar_paus_item"
      title="&foobar.paus;">
      <toolbarbutton id="foobar_paus"
        class="foobarcontrol"
        tooltiptext="&foobar.paus;"
        label="&foobar.toolbar.paus;"
        oncommand="return foobar.docommand('pause');"/>
    </toolbaritem>
    <toolbaritem id="foobar_play_item"
      title="&foobar.play;">
      <toolbarbutton id="foobar_play"
        class="foobarcontrol"
        tooltiptext="&foobar.play;"
        label="&foobar.toolbar.play;"
        oncommand="return foobar.docommand('play');"/>
    </toolbaritem>  
    <toolbaritem id="foobar_prev_item"
      title="&foobar.prev;">
      <toolbarbutton id="foobar_prev"
        class="foobarcontrol"
        tooltiptext="&foobar.prev;"
        label="&foobar.toolbar.prev;"
        oncommand="return foobar.docommand('prev');"/>
    </toolbaritem>
    <toolbaritem id="foobar_next_item"
      title="&foobar.next;">   
      <toolbarbutton id="foobar_next" 
        class="foobarcontrol"
        tooltiptext="&foobar.next;"
        label="&foobar.toolbar.next;"
        oncommand="return foobar.docommand('next');"/>
    </toolbaritem>
    <toolbaritem id="foobar_rand_item"
      title="&foobar.rand;">
      <toolbarbutton id="foobar_rand"
        class="foobarcontrol"
        tooltiptext="&foobar.rand;"
        label="&foobar.toolbar.rand;"
        oncommand="return foobar.docommand('rand');"/>
    </toolbaritem>
  </toolbarpalette>
  
  <toolbarpalette id="MailToolbarPalette">
    <toolbaritem id="foobar_stop_item"
      title="&foobar.stop;">
      <toolbarbutton id="foobar_stop"
        class="foobarcontrol"
        tooltiptext="&foobar.stop;"
        label="&foobar.toolbar.stop;"
        minimised="false"
        oncommand="return foobar.docommand('stop');"/>
    </toolbaritem>
    <toolbaritem id="foobar_paus_item"
      title="&foobar.paus;">
      <toolbarbutton id="foobar_paus"
        class="foobarcontrol"
        tooltiptext="&foobar.paus;"
        label="&foobar.toolbar.paus;"
        oncommand="return foobar.docommand('pause');"/>
    </toolbaritem>
    <toolbaritem id="foobar_play_item"
      title="&foobar.play;">
      <toolbarbutton id="foobar_play"
        class="foobarcontrol"
        tooltiptext="&foobar.play;"
        label="&foobar.toolbar.play;"
        oncommand="return foobar.docommand('play');"/>
    </toolbaritem>  
    <toolbaritem id="foobar_prev_item"
      title="&foobar.prev;">
      <toolbarbutton id="foobar_prev"
        class="foobarcontrol"
        tooltiptext="&foobar.prev;"
        label="&foobar.toolbar.prev;"
        oncommand="return foobar.docommand('prev');"/>
    </toolbaritem>
    <toolbaritem id="foobar_next_item"
      title="&foobar.next;">   
      <toolbarbutton id="foobar_next" 
        class="foobarcontrol"
        tooltiptext="&foobar.next;"
        label="&foobar.toolbar.next;"
        oncommand="return foobar.docommand('next');"/>
    </toolbaritem>
    <toolbaritem id="foobar_rand_item"
      title="&foobar.rand;">
      <toolbarbutton id="foobar_rand"
        class="foobarcontrol"
        tooltiptext="&foobar.rand;"
        label="&foobar.toolbar.rand;"
        oncommand="return foobar.docommand('rand');"/>
    </toolbaritem>
  </toolbarpalette>
   	
</overlay>